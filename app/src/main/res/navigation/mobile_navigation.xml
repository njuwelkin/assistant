<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_chat">

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.assistant.ui.home.HomeFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_course"
        android:name="com.example.assistant.ui.dashboard.DashboardFragment"
        android:label="@string/title_course"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_me"
        android:name="com.example.assistant.ui.notifications.NotificationsFragment"
        android:label="@string/title_me"
        tools:layout="@layout/fragment_notifications" />
</navigation>